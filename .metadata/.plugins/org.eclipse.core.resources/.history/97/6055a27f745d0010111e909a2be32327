package web.model;
import lombok.Data;
import java.util.List;
import jakarta.persistence.*;

@Data
@Entity
@Table(name = "Users")
public class User {
    @Id
    String username;

    String password;

    boolean enabled;

    @OneToMany(mappedBy = "Roles", fetch = FetchType.EAGER)
    List<UserRole> userRoles;
}
